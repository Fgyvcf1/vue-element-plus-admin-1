{
  "name": "vue-element-plus-admin-monorepo",
  "version": "3.0.0",
  "private": true,
  "type": "module",
  "description": "一套基于vue3、element-plus、typesScript、vite的后台集成方案 - Monorepo架构",
  "author": "Archer <502431556@qq.com>",
  "license": "MIT",
  "scripts": {
    "dev": "pnpm --filter @vea/admin dev",
    "build": "pnpm --filter @vea/admin build",
    "build:pro": "pnpm --filter @vea/admin build:pro",
    "build:dev": "pnpm --filter @vea/admin build:dev",
    "build:test": "pnpm --filter @vea/admin build:test",
    "preview": "pnpm --filter @vea/admin preview",
    "ts:check": "pnpm --filter @vea/admin ts:check",
    "lint": "eslint . --fix",
    "lint:lint-staged": "lint-staged -c .husky/lintstagedrc.cjs",
    "lint:style": "stylelint --fix \"**/*.{vue,less,postcss,css,scss}\" --cache",
    "format": "prettier --write .",
    "clean": "pnpm -r clean && rimraf node_modules",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/node": "^22.15.19",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-vue": "^10.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.0.0",
    "postcss": "^8.5.3",
    "postcss-html": "^1.8.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "stylelint": "^16.19.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recommended": "^15.0.0",
    "stylelint-config-standard": "^37.0.0",
    "stylelint-order": "^6.0.4",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.32.1"
  },
  "packageManager": "pnpm@9.15.3",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kailong321200875/vue-element-plus-admin.git"
  },
  "bugs": {
    "url": "https://github.com/kailong321200875/vue-element-plus-admin/issues"
  },
  "homepage": "https://github.com/kailong321200875/vue-element-plus-admin"
}
